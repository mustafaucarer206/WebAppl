@using WebAPP.Models.Sınıflar
@model List<Blog>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Partial3</title>
</head>
<body>
    <div>
        <div class="padding">
            <h4>En Popüler 10 Blog</h4>
           
          
          
            <ul class="list list-pad">
                @{ int i = 0; }
                @foreach (var x in Model)
                {
                    <li class="list-bg"><span>@{i++;}@i.</span><a href="/Blog/BlogDetay/@x.ID">@x.Baslik</a></li>
                }
                </ul>
           

            <a href="#" class="link1">See all hotels</a>
        </div>

    </div>
</body>
</html>
