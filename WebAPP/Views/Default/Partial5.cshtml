@using WebAPP.Models.Sınıflar
@model List<Blog>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Partial5</title>
</head>
<body>
    <div>
        <div class="padding1">
           
            <div class="box1-left">
                @foreach (var x in Model)
                {
                    <div class="bot-img">
                        <img src="@x.BlogImage" alt="" />
                    </div>
                    <div class="bot-desc">
                        <p>@x.Aciklama.Substring(0, 20)...</p>
                    </div>
                    <div class="clear"></div>
                }
            </div>
        </div>
        </div>
</body>
</html>
